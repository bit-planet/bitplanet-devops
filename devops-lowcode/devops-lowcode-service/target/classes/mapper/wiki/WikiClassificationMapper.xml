        <result column="classification_name" property="classificationName" />
        <result column="wiki_sum" property="wikiSum" />
        <result column="introduction" property="introduction" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_user_id,
        update_user_id,
        create_time,
        update_time,
        delete_flag,
        parent_id, classification_name, wiki_sum, introduction
